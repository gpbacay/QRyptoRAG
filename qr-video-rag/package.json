{
  "name": "qr-video-rag",
  "version": "1.0.0",
  "description": "High-density knowledge storage for RAG systems using QR-encoded video. Self-contained solution with no external API dependencies - transform text into searchable MP4 videos.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "qr-video-rag": "bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"examples/**/*.ts\"",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "rag",
    "retrieval-augmented-generation",
    "qr-code",
    "video-storage",
    "knowledge-base",
    "embeddings",
    "vector-database",
    "ai",
    "machine-learning",
    "semantic-search",
    "llm",
    "langchain",
    "llamaindex",
    "high-density-storage"
  ],
  "author": "Gianne Bacay",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gpbacay/qryptorag.git",
    "directory": "qr-video-rag"
  },
  "bugs": {
    "url": "https://github.com/gpbacay/qryptorag/issues"
  },
  "homepage": "https://github.com/gpbacay/qryptorag#readme",
  "dependencies": {
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "jimp": "^1.6.0",
    "jsqr": "^1.4.0",
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/qrcode": "^1.5.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
